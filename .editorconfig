# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
insert_final_newline = true
block_comment_start = /*
block_comment = *
block_comment_end = */  

[*.{js,ts,tsx,jsx,cjs}]
quote_type = single
